{
    "name": "tcg-mobile-api",
    "version": "1.0.0",
    "description": "API REST pour un TCG mobile avec des personnages",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js --inspect=0.0.0.0:9229",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev",
        "prisma:migrate:deploy": "prisma migrate deploy",
        "prisma:studio": "prisma studio",
        "prisma:seed": "node prisma/seed.js",
        "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
        "docker:prod": "docker-compose up -d",
        "docker:stop:dev": "docker-compose -f docker-compose.dev.yml down",
        "docker:stop:prod": "docker-compose down",
        "docker:logs:dev": "docker-compose -f docker-compose.dev.yml logs -f",
        "docker:logs:prod": "docker-compose logs -f"
    },
    "dependencies": {
        "@prisma/client": "^5.4.2",
        "express": "^4.18.2",
        "express-rate-limit": "^6.10.0",
        "helmet": "^7.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "bcryptjs": "^2.4.3",
        "jsonwebtoken": "^9.0.2",
        "joi": "^17.9.2",
        "express-validator": "^7.0.1",
        "multer": "^1.4.5-lts.1",
        "compression": "^1.7.4",
        "morgan": "^1.10.0",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.0"
    },
    "devDependencies": {
        "nodemon": "^3.0.1",
        "prisma": "^5.4.2",
        "@types/node": "^20.6.0"
    },
    "keywords": [
        "tcg",
        "mobile",
        "api",
        "express",
        "prisma",
        "cards"
    ],
    "author": "Alexandre Bernardini",
    "license": "MIT"
}
